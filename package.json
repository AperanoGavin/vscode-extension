{
    "name": "gavin-voice-vscode",
    "publisher": "GavinAperano",
    "icon": "icon.png",
    "displayName": "gavin-voice-vscode",
    "description": "this extension allows you to perform commands with your voice",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:gavin-voice-vscode.helloWorld"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
            "command": "gavin-voice-vscode.helloWorld",
            "title": "Hello World"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.74.0",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "@vscode/test-electron": "^2.2.0",
        "eslint": "^8.28.0",
        "glob": "^8.0.3",
        "mocha": "^10.1.0",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "@google-cloud/speech": "^5.2.0"
    }
}